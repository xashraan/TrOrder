<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="TransferOrder.controller.orderStatusDetail"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:tnt="sap.tnt">
	<App>
		<pages>
			<Page showHeader="false" class="statusPageClass">
				<content>
					<Panel class="orderPagebg">
						<Image src="{Images>/ImagePath}/Accept.png" width="6%" class="acceptIconClass"></Image>
						<ToolbarSpacer/>
						<Label text="Siirtotilaus" class="orderLabelClass"></Label>
						<ToolbarSpacer/>
						<Label text="luotu" class="createdLabelClass"></Label>
						<ToolbarSpacer/>
						<Label text="123456" class="orderNumClass"></Label>
						<ToolbarSpacer/>
						<Text text="00.00.00" class="orderCountClass"></Text>
						<VBox class="vboxClass"></VBox>
					</Panel>
				</content>
				<footer>
					<Toolbar class="toolbarClass">
						<Button text="Luo uusi tilaus" press="onPressCreate" class="orderStatusButton"></Button>
					</Toolbar>
				</footer>
			</Page>
		</pages>
	</App>
</mvc:View>